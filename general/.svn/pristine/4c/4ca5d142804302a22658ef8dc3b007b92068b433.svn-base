.sg_dialog {
	position: fixed;
	z-index: 97;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%
}

.sg_dialog_mask {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	opacity: .6;
	background-color: #000000;
	filter: alpha(opacity = 60)
}

.sg_dialog_content_wrapper {
	position: absolute;
	left: 50%;
	width: 2.7rem;
	min-width: 2.45rem;
	min-height: .4rem;
	border-radius: 10px;
	background-color: white;
	box-shadow: 0 0.05rem 0.2rem rgba(0, 0, 0, 0.2);
	box-shadow: 0 0 0 \9
}

.sg_dialog_content {
	border-radius: 10px
}

.sg_dialog_content_inner {
	display: -webkit-box;
	overflow: hidden;
	width: 2.7rem;
	padding: .28rem 38px .25rem 38px;
	min-height: .65rem;
	line-height: .22rem;
	text-align: center;
	color: #212121;
	-webkit-box-align: center;
	-webkit-box-pack: center;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	box-sizing: border-box
}

.sg_dialog_content_inner.content {
	display: -webkit-box;
	overflow: hidden;
	height: 1.18rem;
	font-size: 17px;
	font-weight: normal;
	line-height: .2rem;
	text-align: center;
	color: #212121;
	-webkit-box-align: center;
	-webkit-box-pack: center
}

.sg_dialog_content_inner.notice_content {
	display: -webkit-box;
	color: #212121;
	font-size: 16px;
	min-height: 1.18rem;
	text-align: justify
}

.btn_wrapper {
	border-bottom-right-radius: 10px;
	border-bottom-left-radius: 10px
}

.btn_wrapper .sg_dialog_button {
	display: inline-block;
	width: 1.35rem;
	padding: .11rem 0;
	font-size: 16px;
	line-height: .22rem;
	text-align: center;
	vertical-align: middle
}

.btn_wrapper .btn_left {
	box-sizing: border-box;
	color: #5F5F5F
}

.btn_wrapper .btn_right {
	box-sizing: border-box;
	margin-left: 0;
	color: #DF4D3B
}

.btn_wrapper .btn_middle {
	display: inline-block;
	width: 100%;
	padding: .11rem 0;
	font-size: 16px;
	line-height: 22px;
	text-align: center;
	vertical-align: middle;
	color: #00AAFD
}

.sg_dialog_confirm .content {
	min-height: 147px;
	height: auto;
	padding: 20px 23px 17px 24px;
	font-size: 16px;
	line-height: 22px;
	color: #212121;
	text-align: left
}

.sg_dialog_confirm .btn_left {
	color: #212121;
	font-size: 16px
}

.sg_dialog_confirm .btn_right {
	color: #00AAFD;
	font-size: 16px
}

